{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run test:ui",
    "test:ui": "cross-env NEXT_PUBLIC_API_URL=http://127.0.0.1:3000 NEXT_SERVICE_TOKEN=test-token vitest run",
    "test:ui:watch": "cross-env NEXT_PUBLIC_API_URL=http://127.0.0.1:3000 NEXT_SERVICE_TOKEN=test-token vitest",
    "test:e2e": "playwright test",
    "test:ally": "cross-env PLAYWRIGHT_ALLY=1 playwright test",
    "review-artifacts": "node ./scripts/review-artifacts.mjs",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "dev:screenshot": "PLAYWRIGHT_BROWSERS_PATH=0 next dev"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.0.3",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
